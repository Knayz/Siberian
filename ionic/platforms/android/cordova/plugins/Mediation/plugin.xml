<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
        id="Mediation"
        version="1.0.0">

    <name>Mediation</name>
    <description>Mediation AdMob.</description>
    <license>Siberian CMS</license>
    <keywords>cordova,siberian</keywords>

    <!-- android -->
    <platform name="android">
        <framework src="com.facebook.android:audience-network-sdk:+" />
        <framework src="com.google.ads.mediation:facebook:+" />
        <framework src="com.startapp:inapp-sdk:4.5.0" />
        <framework src="com.startapp:admob-mediation:2.0.0" />
    </platform>

    <!-- ios -->
    <platform name="ios">
        <framework src="GoogleMobileAdsMediationFacebook" type="podspec" spec="~> 5.1" />
        <framework src="StartAppSDK" type="podspec" spec="~> 4.5.0" />
    </platform>
</plugin>
